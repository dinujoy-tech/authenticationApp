@model List<authApp.Models.EmployeeTask>

<h2>Your Tasks</h2>

<table>
    <thead>
        <tr>
            <th>Task Description</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var task in Model)
        {
            <tr>
                <td>@task.Description</td>
                
            </tr>
        }
    </tbody>
</table>
