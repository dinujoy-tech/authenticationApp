@model IEnumerable<authApp.Models.EmployeeTask>

<h2>Your Assigned Tasks</h2>

<table>
    <thead>
        <tr>
            <th>Task Name</th>
            <th>Description</th>
            <th>Due Date</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var task in Model)
        {
            <tr>
                <td>@task.TaskName</td>
                <td>@task.Description</td>
                <td>@task.DueDate.ToString("d")</td>
                
            </tr>
        }
    </tbody>
</table>
